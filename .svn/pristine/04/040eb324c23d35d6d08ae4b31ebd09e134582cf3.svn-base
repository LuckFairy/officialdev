    <script src="../js/mui.min.js"></script>
    <script src="../js/global.js"></script>
    <script src="../js/mui.lazyload.js"></script>
    <script src="../js/mui.lazyload.img.js"></script>
    <script src="../js/data.config.js"></script>
    <script src="js/assembly.js"></script>
    <script>
    var myDataConfig = dataConfig;
    var myServices = myDataConfig.services;
    (function($) {
        var item2s = document.getElementById('item2');
        document.getElementById('slider').addEventListener('slide', function(e) {
            if (e.detail.slideNumber === 1) {
                if (item2s.querySelector('.mui-loading')) {
                    setTimeout(function() {
                        $.getJSON(myServices, function(data) {
                            var bankCustomer = document.getElementById("bank-customer");
                            bankCustomer.innerHTML = '';
                            bankCustomer.appendChild(createCardFragment(data, 2));
                            $(document).imageLazyload({
                                placeholder: '../images/60x60.gif',
                                diff: true,
                                duration: 600
                            });
                        });
                    }, 600);
                }
            }
        });
        $.getJSON(myServices, function(data) {
            var bankPos = document.getElementById("bank-pos");
            bankPos.appendChild(createCardFragment(data, 1));
            $(document).imageLazyload({
                placeholder: '../images/60x60.gif',
                diff: true,
                duration: 600
            });
        });
    })(mui);
    </script>
</body>

</html>
