                                    <ul class="mui-table-view mui-table-view-customer-dark">
                                        <!--<li class="mui-table-view-cell mui-table-view-cell-customer">
                                            <a class="mui-navigate-right" href="#" id="wallet">
                                                 <i class="fa fa-briefcase" aria-hidden="true"></i> 
