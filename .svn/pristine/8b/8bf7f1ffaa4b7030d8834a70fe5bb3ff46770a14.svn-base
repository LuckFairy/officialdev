		<script type="text/javascript">
			$('#top').load('header.html');
			$('.footer').load('footer.html');
			$(function() {
				var $formCont = $('#resumeForm'),
				$title = $('#resume-title'),
				$i=0,//默认打开个人能力
				$check_flag = false;//验证成功的标识
				function getData(i){
					$.get('public/datas/resumeForm.json',function(data, status, xhr){
						if(status != 'success'){console.log(xhr);alert('数据加载失败');}
						$title.text(data[i].title)
						$formCont.attr('action',data[i].action)
						$.get('public/datas/'+data[i].url+'.txt',function(res){
							$formCont.html(res);
						})
						
					})
				}
				getData($i);
				//保存函数
				function submitForm(opt){
				 var len = $('.tabList li').length-1,
					index = $i;
					opt = opt?'exit' :opt;
					switch(opt){
						case 'previous':index--;if(index>=0){getData(index)}else{index=0;}break;
						case 'next':index++;if(index<=len){getData(index)}else{index=len;}break;
						case 'exit':;
						default:break;
					}
				}
				
				function tab(){
					//tab点击事件
					$('.tabList li').on('click', function() {
						$(this).addClass('active').siblings().removeClass('active');
						var index = $(this).index();
						$i = index;
						getData($i);
					})
				}
				//个人信息验证
				tab()
				
			})
		</script>
